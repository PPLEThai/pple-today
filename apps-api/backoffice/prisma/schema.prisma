generator client {
  provider = "prisma-client-js"
  output   = "../__generated__/prisma"
  engineType = "client"
  previewFeatures = ["queryCompiler"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator prismabox { 
  provider = "prismabox"
  typeboxImportDependencyName = "elysia"
  typeboxImportVariableName = "t"
  inputModel = true
  output   = "../__generated__/prismabox"
} 
